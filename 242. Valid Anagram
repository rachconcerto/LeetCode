#1st edition
class Solution:
    def isAnagram(self, s, t):
        if len(s) != len(t):
            return False
        s_dic = {}
        t_dic = {}
        for i in s:
            if i in t and i not in t_dic:
                s_dic[i] = s.count(i)
                t_dic[i] = t.count(i)       
            elif i not in t:
                return False
            else:
                continue
        if s_dic == t_dic:
            return True
        else:
            return False
