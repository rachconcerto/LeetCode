#1st time 
class Solution:
    def twoSum(self, nums, target):
        check = True
        while check == True:
            for i in range(len(nums)):
                for j in range(i+1, len(nums)):
                    if nums[i] + nums[j] != target:
                        continue
                    elif nums[i] + nums[j] == target:
                        n1 = i
                        n2 = j
                        check = False
                i+=1
        return [n1, n2]
          
